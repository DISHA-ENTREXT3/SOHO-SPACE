{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.sohospace.entrext.in"
        }
      ],
      "destination": "https://sohospace.entrext.in/$1",
      "permanent": true
    }
  ]
}
